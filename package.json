{
  "name": "ckomop0x-me-site",
  "version": "10.0.55",
  "description": "Pavel Klochkov personal blog â€” ckomop0x.me",
  "keywords": [
    "nextjs",
    "graphql",
    "strapi",
    "tailwind"
  ],
  "author": {
    "name": "Pavel Klochkov",
    "email": "paul.klochkov@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://ckomop0x.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/ckomop0x/ckomop0x.me.site.v2"
  },
  "type": "module",
  "scripts": {
    "predev": "pnpm run gen",
    "dev": "next dev --turbopack -p 8000",
    "prebuild": "pnpm run codegen",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --passWithNoTests --coverage",
    "check-style": "prettier --check \"./**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-ts": "tsc",
    "lint": "eslint \"./src/**/*.{ts,tsx,js,jsx}\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "gen": "pnpm run introspect && pnpm run codegen",
    "introspect": "node scripts/introspect.js",
    "rover": "rover",
    "dotenv": "dotenv",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,csgis,md}": [
      "prettier . --write --config ./.prettierrc"
    ],
    "*.{ts,tsx}": [
      "eslint -c ./.eslintrc.js --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.13.9",
    "@apollo/rover": "0.35.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@testing-library/jest-dom": "6.7.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "graphql": "^16.8.1",
    "html-react-parser": "^5.1.10",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-markdown": "^10.0.0",
    "tailwind-merge": "^3.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@next/eslint-plugin-next": "15.4.6",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-links": "9.1.2",
    "@storybook/addon-themes": "9.1.2",
    "@storybook/nextjs": "9.1.2",
    "@stylistic/eslint-plugin": "5.2.3",
    "@tailwindcss/postcss": "4.1.12",
    "@types/node": "24.3.0",
    "@types/react": "19.1.10",
    "@vitejs/plugin-react": "5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "cross-env": "10.0.0",
    "dotenv": "17.2.1",
    "dotenv-cli": "10.0.0",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.2",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.5",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "storybook": "9.1.2",
    "tailwindcss": "4.1.12",
    "ts-graphql-plugin": "4.0.3",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "vitest": "3.2.4"
  },
  "overrides": {
    "eslint": "$eslint",
    "@types/react": "19.1.10",
    "storybook": "$storybook"
  },
  "packageManager": "pnpm@10.14.0"
}
