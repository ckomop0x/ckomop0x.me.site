{
  "name": "ckomop0x-me",
  "version": "2.0.0",
  "description": "Ckomop0x.me site",
  "author": "Pavel Klochkov paul.klochkov@gmail.com",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "develop": "gatsby clean && gatsby develop",
    "getschema": "yarn run apollo schema:download --endpoint=http://localhost:8000/___graphql graphql-schema.json",
    "gettypes": "yarn getschema && yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.tsx --tagName=graphql --addTypename --globalTypesFile=src/types/graphql-global-types.ts --outputFlat=src/types"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.20.36",
    "gatsby-parallel-runner": "^1.2.7",
    "gatsby-plugin-codegen": "^1.1.0",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-plugin-typegen": "^2.1.0",
    "gatsby-plugin-yandex-metrica": "^0.1.3-check",
    "gatsby-source-strapi": "^0.0.12",
    "gatsby-transformer-sharp": "^2.4.3",
    "node-sass": "^4.13.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismic-reactjs": "^1.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "styled-components": "^5.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@types/graphql": "^14.5.0",
    "@types/react-helmet": "^5.0.15",
    "gatsby-plugin-postcss": "^2.2.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-scss-typescript": "^4.0.10",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-graphql": "^2.3.0",
    "html-react-parser": "^0.10.2",
    "prettier": "^2.0.5",
    "ts-graphql-plugin": "^1.12.0",
    "typescript": "^3.9.2"
  }
}
