{
  "name": "ckomop0x-me-site",
  "version": "10.1.83",
  "description": "Pavel Klochkov personal blog â€” ckomop0x.me",
  "keywords": [
    "nextjs",
    "graphql",
    "strapi",
    "tailwind"
  ],
  "author": {
    "name": "Pavel Klochkov",
    "email": "paul.klochkov@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://ckomop0x.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/ckomop0x/ckomop0x.me.site.v2"
  },
  "type": "module",
  "scripts": {
    "predev": "pnpm run gen",
    "dev": "next dev --turbopack -p 8000",
    "prebuild": "pnpm run codegen",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --passWithNoTests --coverage",
    "check-style": "prettier --check \"./**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-ts": "tsc",
    "lint": "eslint \"./src/**/*.{ts,tsx,js,jsx}\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "gen": "pnpm run introspect && pnpm run codegen",
    "introspect": "node scripts/introspect.js",
    "rover": "rover",
    "dotenv": "dotenv",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,csgis,md}": [
      "prettier . --write --config ./.prettierrc"
    ],
    "*.{ts,tsx}": [
      "eslint -c ./.eslintrc.js --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "4.1.0",
    "@apollo/rover": "0.37.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cross-fetch": "^4.1.0",
    "date-fns": "4.1.0",
    "graphql": "^16.8.1",
    "html-react-parser": "^5.1.10",
    "lucide-react": "^0.562.0",
    "next": "16.1.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.0.0",
    "tailwind-merge": "^3.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/client-preset": "5.2.2",
    "@graphql-codegen/introspection": "5.0.0",
    "@next/eslint-plugin-next": "15.5.9",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/addon-themes": "10.1.11",
    "@storybook/nextjs": "10.1.11",
    "@stylistic/eslint-plugin": "5.7.0",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "24.10.9",
    "@types/react": "19.2.8",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "3.2.4",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "dotenv-cli": "11.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.1.11",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.3",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "ts-graphql-plugin": "4.0.3",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vitest": "3.2.4"
  },
  "overrides": {
    "eslint": "$eslint",
    "@types/react": "19.2.8",
    "storybook": "$storybook"
  },
  "packageManager": "pnpm@10.28.0"
}
